/* pages/myQRCode/index.wxss */
.container{
  margin: 0;
  padding: 0;
  background: #FFFFFF;
}
.code{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100vh;
}
.code .bg{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 14rem;
  height: 14rem;
  padding: 0.5rem;
  box-sizing: border-box;
  border-radius: 50%;
  background: #474747;
}
.code image{
  width: 10rem;
  height: 10rem;
}
